        <div class="row">
            <div class="col-lg-12 col-md-12 popupdata">
                <label class="close" data-dismiss="modal" style="margin-right:8px;">&times;</label>
                <div class="dashboard-list-box margin-top-5">
                    <h4 id="">IT Skills</h4>
                    <div class="dashboard-list-box-content">
                        <div class="submit-page">
                            <input type="hidden" name="ITSkillId" id="ITSkillId" value="0" />
                            <div class="form" style="width:20%">
                                <input type="text" name="ITSkill" id="ITSkill" class="search-field eduModeltxtbox" placeholder="Skill">
                            </div>
                            <div class="form" style="width:20%">
                                <input type="text" name="SkillVersion" id="SkillVersion" class="search-field eduModeltxtbox" maxlength="10" placeholder="Skill Version">
                            </div>
                            <div class="form" style="width:20%">
                                <select name="LastUsed" id="LastUsed" class="selectDropdown">
                                    <option value="" disabled selected>Last Used</option>
                                    @{
                                        int nowYear = DateTime.Now.Year;
                                        @for (int i = (nowYear - 10); i <= nowYear; i++)
                                        {
                                            <option value="@i">@i</option>
                                        }
                                    }
                                </select>
                            </div>
                            <div class="form" style="width:20%">
                                <select name="ExperienceYear" id="ExperienceYear" class="selectDropdown">
                                    <option value="" disabled selected>Exp. Years</option>
                                    @for (int i = 0; i <= 11; i++)
                                    {
                                        <option value=@i>@(i)</option>
                                    }
                                </select>
                            </div>
                            <div class="form" style="width:20%">
                                <select name="ExperienceMonth" id="ExperienceMonth" class="selectDropdown">
                                    <option value="" disabled selected>Exp. Months</option>
                                    @for (int i = 0; i <= 11; i++)
                                    {
                                        <option value=@i>@(i) Month(s)</option>
                                    }
                                </select>
                            </div>
                            <div class="form">
                                <input type="submit" class="button border fw margin-top-10 pull-right" onclick="UpdateITSKill();" value="Save changes" />
                            </div>
                            <div class="form">
                                <input type="button" class="button border fw margin-top-10 " data-dismiss="modal" id="closeModelPupuppSummary" value="Close" />
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
