@*@{
    ViewData["Title"] = "Index";
}
<!--map js-->
<link href="~/css/Admin/jqvmap.css" rel='stylesheet' type='text/css' />
<!-- //map js -->
<div id="page-wrapper" style="min-height: 262px;">
    <h3 class="title1">Welcome to Dashboard</h3>
    <div class="main-page">
        <div class="row-one">
            <div class="col-md-4 widget">
                <div class="stats-left ">
                    <h5>Today</h5>
                    <h4>Employer Registration</h4>
                </div>
                <div class="stats-right" id="Employerdaywise">
                    <label id="lblEmpRegistration"></label>
                </div>
                <div class="clearfix"> </div>
            </div>
            <div class="col-md-4 widget states-mdl">
                <div class="stats-left">
                    <h5>Today</h5>
                    <h4>Job Post</h4>
                </div>
                <div class="stats-right" id="jobPostDatewise">
                    <label id="lblJobPost"></label>
                </div>
                <div class="clearfix"> </div>
            </div>
            <div class="col-md-4 widget states-last">
                <div class="stats-left">
                    <h5>Today</h5>
                    <h4>Resume Post</h4>
                </div>
                <div class="stats-right" id="resumePost">
                    <label id="lblResumePost"></label>
                </div>
                <div class="clearfix"> </div>
            </div>
            <div class="clearfix"> </div>
        </div><br />
        <div class="row-one">
            <div class="col-md-4 widget">
                <div class="stats-left ">
                    <h5>Today</h5>
                    <h4>Staffing Partner</h4>
                </div>
                <div class="stats-right" id="stuffToday">
                    <label id="lblStuffingPartner"></label>
                </div>
                <div class="clearfix"> </div>
            </div>
            <div class="col-md-4 widget states-mdl">
                <div class="stats-left">
                    <h5>Today</h5>
                    <h4>Training Partner</h4>
                </div>
                <div class="stats-right" id="tpToday">
                    <label id="lblTraningPartner"> </label>
                </div>
                <div class="clearfix"> </div>
            </div>
            <div class="col-md-4 widget states-last">
                <div class="stats-left">
                    <h5>Today</h5>
                    <h4>Student</h4>
                </div>
                <div class="stats-right" id="studentdaywise">
                    <label id="lblStudentRegistration"></label>
                </div>
                <div class="clearfix"> </div>
            </div>
            <div class="clearfix"> </div>
        </div>
        <div class="charts">
            <div class="col-md-4 charts-grids widget">
                <h4 class="title">Job Post Month Wise</h4>

                <canvas id="bar" height="300" width="400"> </canvas>
            </div>
            <div class="col-md-4 charts-grids widget states-mdl">
                <h4 class="title">Applied Job Month Wise</h4>
                <canvas id="line"> </canvas>
            </div>
            <div class="col-md-4 charts-grids widget">
                <h4 class="title">Month Wise User Registration</h4>
                <canvas id="pie" height="300" width="400"> </canvas>
            </div>
            <div class="clearfix"> </div>
        </div>


    </div>

</div>
@section Scripts
    {
   
    <script src="~/js/Admin/Chart.js"></script>
    <script src="~/js/Admin/jquery.vmap.js"></script>
    <script src="~/js/Admin/jquery.vmap.sampledata.js"></script>
    <script src="~/js/Admin/jquery.vmap.world.js"></script>
    <script src="~/js/Admin/Index/Index.js"></script>


}*@