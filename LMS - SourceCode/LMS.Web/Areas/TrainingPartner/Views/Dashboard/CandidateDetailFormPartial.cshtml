@using LMS.Model.DataViewModel.TrainingPartner
@model CandidatesViewModel

<div class="row col-md-12">
    <form>
        <input type="hidden" name="Id" value="@Model.Id" />
        <div class="form-group row">
            <label class="col-md-2" for="CandidateId">Candidate Id</label>
            <div class="col-sm-10">
                <input type="text" readonly class=" form-control" name="CandidateId" value="@Model.CandidateId" id="candidateId" />
            </div>
        </div>
        <div class="form-group row">
            <label class="col-md-2" for="FirstName">First Name</label>
            <div class="col-sm-10">
                <input type="text" class=" form-control" name="FirstName" value="@Model.FirstName" id="firstname" />
            </div>
        </div>
        <div class="form-group row">
            <label class="col-md-2" for="LastName">Last Name</label>
            <div class="col-sm-10">
                <input type="text" class=" form-control" name="LastName" value="@Model.LastName" id="lastname" />
            </div>
        </div>
        <div class="form-group row">
            <label class="col-md-2" for="Email">Email</label>
            <div class="col-sm-10">
                <input type="text" readonly class=" form-control" name="Email" value="@Model.Email" id="email" />
            </div>
        </div>
        <div class="form-group row">
            <label class="col-md-2" for="Password">Password</label>
            <div class="col-sm-10">
                <input type="password" readonly class=" form-control" name="Password" value="@Model.Password" id="password" />
            </div>
        </div>
    </form>
</div>
